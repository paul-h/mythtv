<platform>
    <name>Freesat</name>
    <types>
        <type>DVB-S</type>
        <type>DVB-S2</type>
    </type>
    <countries>
        <country>gb</country>
        <country>ie</country>
    </countries>
    <transports> <!-- For most networks only one is needed -->
        <transport fullSI="false">
            <frequency>10714000</frequency> <!-- Khz -->
            <symbolrate>22000000</symbolrate>
            <polarisation>horizontal</polarisation>
            <fec>5/6</fec>
            <rolloff>0.35</rolloff>
        </transport>
    </transports>
</platform>
