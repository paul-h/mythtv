    <window name="chartviewer">

        <textarea name="title_text">
            <area>0,10,1280,50</area>
            <font>baselarge</font>
            <align>allcenter</align>
            <value></value>
        </textarea>

        <shape name="top_background" from="basebackground">
            <area>35,65,1210,180</area>
        </shape>

        <textarea name="date_text" from="basetextarea">
            <area>40,10,200,80</area>
            <font>basesmall</font>
            <multiline>yes</multiline>
            <value></value>
        </textarea>

        <textarea name="time_text" from="basetextarea">
            <area>1040,10,200,40</area>
            <font>basesmall</font>
            <align>right</align>
            <value></value>
        </textarea>

        <buttonlist name="chart_list">
            <area>145,75,1200,160</area>
            <layout>horizontal</layout>
            <spacing>9</spacing>
            <buttonarea>45,0,1100,160</buttonarea>
            <statetype name="buttonitem">
                <state name="active">
                    <area>0,0,220,160</area>
                    <shape name="buttonbackground">
                        <area>0,0,220,160</area>
                        <fill style="gradient">
                            <gradient start="#505050" end="#000000" alpha="200" />
                        </fill>
                    </shape>
                    <textarea name="buttontext" from="basetextarea">
                        <area>5,130,210,30</area>
                        <font>basesmallpurple</font>
                        <align>hcenter,top</align>
                    </textarea>
                    <imagetype name="buttonimage">
                        <area>10,10,200,115</area>
                        <preserveaspect>false</preserveaspect>
                        <filename>preview.png</filename>
                    </imagetype>
                </state>
                <state name="selectedactive" from="active">
                    <shape name="buttonbackground">
                        <fill style="gradient">
                            <gradient start="#52CA38" end="#349838" alpha="255" />
                        </fill>
                    </shape>
                    <textarea name="buttontext">
                        <font>basesmall</font>
                    </textarea>

                </state>
            </statetype>
            <statetype name="upscrollarrow">
                <position>0,65</position>
                <state type="on">
                    <imagetype name="upon">
                        <filename>ui/left_arrow-sel.png</filename>
                    </imagetype>
                </state>
                <state type="off">
                    <imagetype name="upon">
                        <filename>ui/left_arrow-reg.png</filename>
                    </imagetype>
                </state>
            </statetype>
            <statetype name="downscrollarrow">
                <position>960,65</position>
                <state type="on">
                    <imagetype name="dnon">
                        <filename>ui/right_arrow-sel.png</filename>
                    </imagetype>
                </state>
                <state type="off">
                    <imagetype name="upon">
                        <filename>ui/right_arrow-reg.png</filename>
                    </imagetype>
                </state>
            </statetype>
        </buttonlist>

        <shape name="track_info_background" from="basebackground">
            <area>35,250,1210,450</area>
        </shape>

        <textarea name="nochart" from="basetextarea">
            <area>45,215,1185,415</area>
            <align>allcenter</align>
            <font>basemedium</font>
            <value>Chart Not Available</value>
        </textarea>

        <imagetype name="chart_image">
            <area>45,260,1190,430</area>
            <filename></filename>
        </imagetype>

  </window>

      <window name="customchartdialog">
        <area>-1,-1,500,500</area>

        <imagetype name="backimg">
            <area>0,0,100%,100%</area>
            <filename>mythdialogbox-background.png</filename>
        </imagetype>

        <textarea name="label" from="basetextarea">
            <area>20,10,100%-20,60</area>
            <font>baselarge</font>
            <multiline>yes</multiline>
            <align>allcenter</align>
            <value>Custom Chart Parameters</value>
        </textarea>

        <textarea name="daylbl"  from="basetextarea">
            <area>90,70,170,30</area>
            <align>left,vcenter</align>
            <position>50,125</position>
            <value>Day</value>
        </textarea>

        <spinbox name="day" from="basespinbox">
            <position>50,153</position>
        </spinbox>

        <textarea name="monthlbl"  from="daylbl">
            <position>200,125</position>
            <value>Month</value>
        </textarea>

        <spinbox name="month" from="basespinbox">
            <position>200,153</position>
        </spinbox>

        <textarea name="yearlbl"  from="daylbl">
            <position>350,125</position>
            <value>Year</value>
        </textarea>

        <spinbox name="year" from="basespinbox">
            <position>350,153</position>
        </spinbox>

        <textarea name="periodlabel" from="basetextarea">
            <area>58,255,90,30</area>
            <align>right,vcenter</align>
            <value>Period:</value>
        </textarea>

        <buttonlist name="period" from="baseselector">
            <position>155,250</position>
        </buttonlist>

        <button name="ok" from="basebutton">
            <position>50%-77,420</position>
            <value>OK</value>
        </button>
    </window>

