<mythuitheme>

    <window name="currentconditions">

        <font name="title" from="basesmall">
            <color>#ffff00</color>
            <dropcolor>#000000</dropcolor>
            <size>24</size>
            <shadow>3,3</shadow>
            <bold>yes</bold>
        </font>

        <font name="heading" from="basesmall">
            <color>#ffff00</color>
            <size>18</size>
            <shadow>2,2</shadow>
            <bold>yes</bold>
        </font>

        <font name="label" from="basesmall">
            <color>#ffff00</color>
            <size>15</size>
            <bold>yes</bold>
        </font>

        <font name="date" from="basesmall">
            <color>#9999cc</color>
            <size>14</size>
            <bold>no</bold>
        </font>

        <font name="info" from="basesmall">
            <color>#ffffff</color>
            <size>14</size>
            <bold>no</bold>
        </font>

        <textarea name="title" from="basetextarea">
            <area>20,10,760,50</area>
